    "browserify": "^14.1.0",
    "chai": "*",
    "es6-promise": "^3.0.2",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-concat": "^1.0.1",
